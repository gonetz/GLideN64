                </property>
                <item>
                 <property name="text">
                  <string comment="vi">Vertical interrupt (recommended, fewest game issues)</string>
                 </property>
                </item>
                <item>
                 <property name="text">
                  <string comment="origin">VI origin change (faster, few game issues)</string>
                 </property>
                </item>
                <item>
                 <property name="text">
                  <string comment="colorBuffer">Color buffer change (fastest, some game issues)</string>
                 </property>
                </item>
               </widget>
              </item>
             </layout>
            </item>
